<!-- Header -->
<%- include('./partials/headerLogueado.ejs') %>
<!-- /Header -->


<!-- Novedades -->
<div class="container products-wrapper">
	<div class="row">
		<div class="col-12">
			<h1 class="products-title">Novedades</h1>
		</div>
	<% for (let i=0; i<searchResults.length; i++){ %>

	<div class="col-12 col-sm-6 col-lg-3">
			<section class="product-box">
				<a href="/productos/especifico/<%= searchResults[i].id %>">
					<figure class="product-box_image">
						<img src="<%=searchResults[i].imagen %>" alt="cafetera moulinex">
					</figure>
					<article class="product-box_data">
						<h2> <%= searchResults[i].nombreDelProducto %></h2>
						<p><%= searchResults[i].descripcion %></p>
						<p><%= searchResults[i].fechaDeCarga %></p>
					</article>
				</a>
			</section>
		</div>
		<%}	 %>
	</div>
</div>
<!-- /Novedades -->